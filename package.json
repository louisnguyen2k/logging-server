{
  "name": "logging",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && rm -rf logs",
    "start": "node dist/index.js",
    "devlog": "tsc -p . && ts-node-dev dist/index.js",
    "dev": "tsoa spec-and-routes && concurrently \"nodemon\" \"nodemon --watch './**/controllers/*.ts' -x tsoa spec-and-routes\"",
    "dev:win": "tsoa routes && concurrently  \"nodemon\" \"nodemon --watch './**/controllers/*.ts' -x tsoa spec-and-routes\"",
    "dev:ignored": "nodemon --watch './**/*.ts'--delay 3  --exec 'ts-node' src/index.ts",
    "dev:tsoa": "tsoa routes && concurrently  \"nodemon --exec 'ts-node src/index.ts'\" \"nodemon -x tsoa spec\"",
    "dev:auto": "tsoa spec-and-routes && concurrently \"nodemon\" \"nodemon --watch './**/controllers/*.ts' -x tsoa spec-and-routes\"",
    "build:code": "tsc -p .",
    "build:route": "tsoa routes",
    "build:doc": "tsoa spec-and-routes",
    "build": "yarn build:doc && yarn build:code",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "body-parser": "~1.17.0",
    "cookie-parser": "^1.4.5",
    "cors": "~2.8.1",
    "cross-env": "~3.2.3",
    "dotenv": "^6.1.0",
    "express": "~4.15.0",
    "express-session": "^1.17.2",
    "express-subdomain": "^1.0.5",
    "express-useragent": "^1.0.15",
    "express-validator": "^6.12.1",
    "install": "^0.13.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node-html-parser": "^3.3.4",
    "npm": "^7.20.6",
    "request": "^2.88.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/express-validator": "^3.0.0",
    "@types/morgan": "^1.9.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.1.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsoa": "^3.7.0",
    "typescript": "^4.2.3",
    "typescript-formatter": "^7.2.2"
  },
  "_moduleAliases": {
    "@config": "./dist/config",
    "@controllers": "./dist/controllers",
    "@models": "./dist/models",
    "@routes": "./dist/routes",
    "@services": "./dist/services",
    "@utils": "./dist/utils",
    "@helpers": "./dist/helpers",
    "@commons": "./dist/commons",
    "@middleware": "./dist/middleware"
  }
}
